import java.util.Scanner;

public class a1_q5 {
     public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int x = scn.nextInt();
        int dig = 0;
        while (x!=0){
            x=x/10;
            dig++;
        }
        System.out.println(dig);
        scn.close();
    }        
}
